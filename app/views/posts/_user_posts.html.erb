
<ul>
  <% unless @posts.nil? %>
    <% @posts.each do |post| %>
      <% if post.user_id == @user.id %>
        <li>
          <p>Author: <%= User.find(post.user_id).username %></p>
          <p>Post: <%= post.body %></p>
          <p>Created at: <%= post.created_at %></p>
        </li>
      <% end %>
    <% end %>
  <% end %>
</ul>
